<Window x:Class="DemoHeatmapGUI.InstallMapDialogue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoHeatmapGUI"
        mc:Ignorable="d"
        Title="InstallMapDialogue" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="128"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Image x:Name="target_previewImage"/>
        <Label x:Name="target_mapname" Grid.Row="1"/>
    </Grid>
</Window>
